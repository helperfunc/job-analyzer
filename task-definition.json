{
  "family": "job-analyzer",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "job-analyzer",
      "image": "076181803263.dkr.ecr.us-east-1.amazonaws.com/job-analyzer:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "HOSTNAME",
          "value": "0.0.0.0"
        },
        {
          "name": "NEXT_TELEMETRY_DISABLED",
          "value": "1"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/database-url-oSOWVT"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/jwt-secret-wxe5Ds"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/google-client-id-FtIAJB"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/google-client-secret-JojSUb"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/supabase-url-3fqNi9"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:076181803263:secret:job-analyzer/supabase-anon-key-tiVJBJ"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/job-analyzer",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::076181803263:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::076181803263:role/ecsTaskRole"
}
